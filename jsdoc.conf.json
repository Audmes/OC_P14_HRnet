{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["./src", "./README.md"]
	},
	"plugins": ["plugins/markdown", "better-docs/component", "better-docs/category"],
	"opts": {
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true,
		"template": "./node_modules/better-docs"
	},
	"templates": {
		"better-docs": {
			"name": "HRnet - Documentation"
		}
	}
}
